#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long n,a,c,b;
    cin>>n;
    vector<long> v;
    if(n==0){
        cout<<0;
    }
    else{
    while(n>0){
        a=n%10;
        if(a==0){
            v.push_back(0);
        }
        else{
            v.push_back(a);
        }
        n=n/10;
        
    }
    for(int i=0; i<v.size(); i++){
        cout<<v[i];
    }
}
}
