#include <bits/stdc++.h>

using namespace std;

int main() {
    double a, b, c;
    

    cin >> a >> b >> c;

    double s = (a + b + c) / 2;

 
    double area = sqrt(s * (s - a) * (s - b) * (s - c));

    double h_a = 2 * area / a;
    double h_b = 2 * area / b;
    double h_c = 2 * area / c;

    cout << fixed << setprecision(2) << h_a << " " << h_b << " " << h_c << endl;

}
