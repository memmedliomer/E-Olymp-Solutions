#include <bits/stdc++.h>

using namespace std;

int main()
{
  double x,y;
  cin>>fixed>>setprecision(2)>>x>>y;
  if(x>0 and y>0){
      cout<<"1"<<endl;
  }
  else if(x>0 and y<0){
    cout<<"4"<<endl;
}
 else if(x<0 and y>0){
     cout<<"2"<<endl;
 }
 else if(x<0 and y<0){
     cout<<"3"<<endl;
 }
 else {
     cout<<"0"<<endl;
 }
       
       
   }
