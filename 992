#include <bits/stdc++.h>
using namespace std;
int main()
{
    long long  a,b,c,d,e,cem=0;
    int n;
    cin >>n;
    vector<long> v;
   long long A[n+1][n+1];
    for(long i=0; i<n; i++){
        for(long j=0; j<n; j++){
            cin>>A[i][j];
             cem=cem+A[i][j];
             
        }
       
    }
    cout << cem / 2;
    
}
