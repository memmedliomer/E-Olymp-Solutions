#include <bits/stdc++.h>

using namespace std;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	cout.tie(0);
    long long a,b,c,d=1,f=0,g;
    set<char> s;
    string s1;
    getline(cin,s1);
    vector<char> v;
    for(long i=0; i<s1.size(); i++){
    	if(isdigit(s1[i])){
    		s.insert(s1[i]);
		}
	}
	for(char i='0'; i<='9'; i++){
		if(s.find(i)==s.end()){
			v.push_back(i);
			
		}
	}
	cout<<v.size()<<endl;
	for(int i=0; i<v.size(); i++){
		cout<<v[i]<<" ";
	}
	
	}
