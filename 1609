#include <bits/stdc++.h>
using namespace std;
int main() {
   string s;
   char a;
   int say = 0;
   cin>>s >> a;
   if(s[0] == '-')
   {
   	for(char i=1; i<s.size(); i++)
   {
   	if(s[i] == a)
   	{
   		say++;
	   }
   }
   cout << say << endl;
   }
   else
   {
   	 	for(char i=0; i<s.size(); i++)
   {
   	if(s[i] == a)
   	{
   		say++;
	   }
   }
   cout << say << endl;
   
   }
}
